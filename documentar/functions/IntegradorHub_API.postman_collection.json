    {
        "info": {
            "name": "IntegradorHub.API - Collection",
            "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
            "version": "1.0.0"
        },
        "variable": [
            { "key": "baseUrl", "value": "http://localhost:5000" },
            { "key": "token", "value": "" }
        ],
        "item": [
            {
                "name": "Auth",
                "item": [
                            {
                                "name": "Login",
                                "request": {
                            "method": "POST",
                            "header": [{ "key": "Content-Type", "value": "application/json" }],
                            "body": {
                                "mode": "raw",
                                "raw": "{\n  \"FirebaseUid\": \"{{FirebaseUid}}\",\n  \"Email\": \"<EDIT>\",\n  \"DisplayName\": \"<EDIT>\",\n  \"PhotoUrl\": \"<EDIT>\"\n}",
                                "options": { "raw": { "language": "json" } }
                            },
                            "url": { "raw": "{{baseUrl}}/api/auth/login", "host": ["{{baseUrl}}"], "path": ["api", "auth", "login"] }
                        }
                    },
                    {
                        "name": "Register",
                        "request": {
                            "method": "POST",
                            "header": [{ "key": "Content-Type", "value": "application/json" }],
                            "body": {
                                "mode": "raw",
                                "raw": "{\n  \"FirebaseUid\": \"{{FirebaseUid}}\",\n  \"Email\": \"<EDIT>\",\n  \"Nombre\": \"<EDIT>\",\n  \"ApellidoPaterno\": \"<EDIT>\",\n  \"ApellidoMaterno\": \"<EDIT>\",\n  \"Rol\": \"<EDIT>\",\n  \"GrupoId\": \"\",\n  \"Matricula\": \"<EDIT>\",\n  \"CarreraId\": \"\",\n  \"Profesion\": \"<NULLABLE>\",\n  \"Organizacion\": \"<NULLABLE>\",\n  \"Asignaciones\": [],\n  \"GruposDocente\": [],\n  \"CarrerasIds\": []\n}",
                                "options": { "raw": { "language": "json" } }
                            },
                            "url": { "raw": "{{baseUrl}}/api/auth/register", "host": ["{{baseUrl}}"], "path": ["api", "auth", "register"] }
                        }
                    }
                ]
            },
            {
                "name": "Projects",
                "item": [
                    {
                        "name": "Get Public Projects",
                        "request": { "method": "GET", "url": { "raw": "{{baseUrl}}/api/projects/public", "host": ["{{baseUrl}}"], "path": ["api", "projects", "public"] } }
                    },
                    {
                        "name": "Create Project",
                        "request": {
                            "method": "POST",
                            "header": [
                                { "key": "Content-Type", "value": "application/json" },
                                { "key": "Authorization", "value": "Bearer {{token}}" }
                            ],
                            "body": {
                                "mode": "raw",
                                "raw": "{\n  \"Titulo\": \"<EDIT>\",\n  \"Materia\": \"<EDIT>\",\n  \"MateriaId\": \"\",\n  \"Ciclo\": \"<EDIT>\",\n  \"StackTecnologico\": [],\n  \"RepositorioUrl\": \"<EDIT>\",\n  \"VideoUrl\": \"<EDIT>\",\n  \"UserId\": \"\",\n  \"UserGroupId\": \"\",\n  \"DocenteId\": \"\",\n  \"MiembrosIds\": []\n}",
                                "options": { "raw": { "language": "json" } }
                            },
                            "url": { "raw": "{{baseUrl}}/api/projects", "host": ["{{baseUrl}}"], "path": ["api", "projects"] }
                        }
                    },
                    {
                        "name": "Get Project by Id",
                        "request": { "method": "GET", "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }], "url": { "raw": "{{baseUrl}}/api/projects/:id", "host": ["{{baseUrl}}"], "path": ["api", "projects", ":id"] } }
                    },
                    {
                        "name": "Update Project",
                        "request": {
                            "method": "PUT",
                            "header": [{ "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{token}}" }],
                            "body": { "mode": "raw", "raw": "{\n  \"Titulo\": \"<EDIT>\",\n  \"VideoUrl\": \"<EDIT>\",\n  \"CanvasBlocks\": [],\n  \"EsPublico\": false\n}", "options": { "raw": { "language": "json" } } },
                            "url": { "raw": "{{baseUrl}}/api/projects/:id", "host": ["{{baseUrl}}"], "path": ["api", "projects", ":id"] }
                        }
                    },
                    {
                        "name": "Delete Project",
                        "request": { "method": "DELETE", "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }], "url": { "raw": "{{baseUrl}}/api/projects/:id", "host": ["{{baseUrl}}"], "path": ["api", "projects", ":id"] } }
                    },
                    {
                        "name": "Add Member",
                        "request": {
                            "method": "POST",
                            "header": [{ "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{token}}" }],
                            "body": { "mode": "raw", "raw": "{\n  \"LeaderId\": \"\",\n  \"EmailOrMatricula\": \"<EDIT>\"\n}", "options": { "raw": { "language": "json" } } },
                            "url": { "raw": "{{baseUrl}}/api/projects/:id/members", "host": ["{{baseUrl}}"], "path": ["api", "projects", ":id", "members"] }
                        }
                    },
                    {
                        "name": "Remove Member",
                        "request": { "method": "DELETE", "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }], "url": { "raw": "{{baseUrl}}/api/projects/:id/members/:memberId", "host": ["{{baseUrl}}"], "path": ["api", "projects", ":id", "members", ":memberId"] } }
                    },
                    {
                        "name": "Update Canvas",
                        "request": { "method": "PUT", "header": [{ "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{token}}" }], "body": { "mode": "raw", "raw": "{\n  \"Blocks\": [],\n  \"UserId\": \"\"\n}", "options": { "raw": { "language": "json" } } }, "url": { "raw": "{{baseUrl}}/api/projects/:id/canvas", "host": ["{{baseUrl}}"], "path": ["api", "projects", ":id", "canvas"] } }
                    }
                ]
            },
            {
                "name": "Evaluations",
                "item": [
                    {
                        "name": "Create Evaluation",
                        "request": {
                            "method": "POST",
                            "header": [{ "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{token}}" }],
                            "body": { "mode": "raw", "raw": "{\n  \"ProjectId\": \"\",\n  \"DocenteId\": \"\",\n  \"DocenteNombre\": \"<EDIT>\",\n  \"Tipo\": \"<EDIT>\",\n  \"Contenido\": \"<EDIT>\",\n  \"Calificacion\": 0\n}", "options": { "raw": { "language": "json" } } },
                            "url": { "raw": "{{baseUrl}}/api/evaluations", "host": ["{{baseUrl}}"], "path": ["api", "evaluations"] }
                        }
                    },
                    {
                        "name": "Get Evaluations By Project",
                        "request": { "method": "GET", "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }], "url": { "raw": "{{baseUrl}}/api/evaluations/project/:projectId", "host": ["{{baseUrl}}"], "path": ["api", "evaluations", "project", ":projectId"] } }
                    }
                ]
            },
            {
                "name": "Storage",
                "item": [
                    {
                        "name": "Upload File",
                        "request": {
                            "method": "POST",
                            "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
                            "body": { "mode": "formdata", "formdata": [{ "key": "file", "type": "file", "src": [] }] },
                            "url": { "raw": "{{baseUrl}}/api/storage/upload?folder=projects", "host": ["{{baseUrl}}"], "path": ["api", "storage", "upload"], "query": [{ "key": "folder", "value": "projects" }] }
                        }
                    },
                    {
                        "name": "Upload Multiple",
                        "request": {
                            "method": "POST",
                            "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
                            "body": { "mode": "formdata", "formdata": [{ "key": "files", "type": "file", "src": [] }] },
                            "url": { "raw": "{{baseUrl}}/api/storage/upload-multiple?folder=projects", "host": ["{{baseUrl}}"], "path": ["api", "storage", "upload-multiple"], "query": [{ "key": "folder", "value": "projects" }] }
                        }
                    },
                    {
                        "name": "Delete File",
                        "request": { "method": "DELETE", "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }], "url": { "raw": "{{baseUrl}}/api/storage/{filePath}", "host": ["{{baseUrl}}"], "path": ["api", "storage", "{filePath}"] } }
                    }
                ]
            },
            {
                "name": "Teams",
                "item": [
                    {
                        "name": "Available Students",
                        "request": {
                            "method": "GET",
                            "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
                            "url": { "raw": "{{baseUrl}}/api/teams/available-students?groupId=", "host": ["{{baseUrl}}"], "path": ["api", "teams", "available-students"], "query": [{ "key": "groupId", "value": "" }] }
                        }
                    },
                    {
                        "name": "Available Teachers",
                        "request": {
                            "method": "GET",
                            "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
                            "url": { "raw": "{{baseUrl}}/api/teams/available-teachers?groupId=", "host": ["{{baseUrl}}"], "path": ["api", "teams", "available-teachers"], "query": [{ "key": "groupId", "value": "" }] }
                        }
                    }
                ]
            },
            {
                "name": "Admin",
                "item": [
                    {
                        "name": "Seed Admin",
                        "request": {
                            "method": "POST",
                            "header": [{ "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{token}}" }],
                            "body": { "mode": "raw", "raw": "{\n  \"Uid\": \"\",\n  \"Email\": \"<EDIT>\"\n}", "options": { "raw": { "language": "json" } } },
                            "url": { "raw": "{{baseUrl}}/api/admin/seed-admin", "host": ["{{baseUrl}}"], "path": ["api", "admin", "seed-admin"] }
                        }
                    },
                    {
                        "name": "Carreras - Create",
                        "request": { "method": "POST", "header": [{ "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{token}}" }], "body": { "mode": "raw", "raw": "{\n  \"Nombre\": \"<EDIT>\",\n  \"Nivel\": \"<EDIT>\"\n}", "options": { "raw": { "language": "json" } } }, "url": { "raw": "{{baseUrl}}/api/admin/carreras", "host": ["{{baseUrl}}"], "path": ["api", "admin", "carreras"] } }
                    },
                    {
                        "name": "Groups - Create",
                        "request": { "method": "POST", "header": [{ "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{token}}" }], "body": { "mode": "raw", "raw": "{\n  \"Nombre\": \"<EDIT>\",\n  \"Carrera\": \"<EDIT>\",\n  \"Turno\": \"<EDIT>\",\n  \"CicloActivo\": \"<EDIT>\"\n}", "options": { "raw": { "language": "json" } } }, "url": { "raw": "{{baseUrl}}/api/admin/groups", "host": ["{{baseUrl}}"], "path": ["api", "admin", "groups"] } }
                    },
                    {
                        "name": "Materias - Create",
                        "request": { "method": "POST", "header": [{ "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{token}}" }], "body": { "mode": "raw", "raw": "{\n  \"Nombre\": \"<EDIT>\",\n  \"Clave\": \"<EDIT>\",\n  \"CarreraId\": \"\",\n  \"Cuatrimestre\": 0,\n  \"EsAltaPrioridad\": false\n}", "options": { "raw": { "language": "json" } } }, "url": { "raw": "{{baseUrl}}/api/admin/materias", "host": ["{{baseUrl}}"], "path": ["api", "admin", "materias"] } }
                    },
                    {
                        "name": "Users - Update Student Group",
                        "request": { "method": "PUT", "header": [{ "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{token}}" }], "body": { "mode": "raw", "raw": "{\n  \"GrupoId\": \"\"\n}", "options": { "raw": { "language": "json" } } }, "url": { "raw": "{{baseUrl}}/api/admin/users/students/:userId", "host": ["{{baseUrl}}"], "path": ["api", "admin", "users", "students", ":userId"] } }
                    },
                    {
                        "name": "Users - Update Teacher Assignments",
                        "request": { "method": "PUT", "header": [{ "key": "Content-Type", "value": "application/json" }, { "key": "Authorization", "value": "Bearer {{token}}" }], "body": { "mode": "raw", "raw": "{\n  \"Asignaciones\": [{ \"CarreraId\": \"\", \"MateriaId\": \"\", \"GruposIds\": [] }]\n}", "options": { "raw": { "language": "json" } } }, "url": { "raw": "{{baseUrl}}/api/admin/users/teachers/:userId", "host": ["{{baseUrl}}"], "path": ["api", "admin", "users", "teachers", ":userId"] } }
                    }
                ]
            }
        ]
    }

